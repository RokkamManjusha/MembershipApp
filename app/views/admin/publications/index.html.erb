<h2>
	Publications
</h2>
<%= link_to "New",new_admin_publication_path,class: "btn btn-primary" %>

<% @publications.each do |publication| %>
<div class="panel panel-default">
	<div class="panel-heading">
		<h3 class="panel-title"><%= publication.title %></h3>
	</div>
	<div class="panel-body">
	<p><%= publication.description %></p>
	<p><%= publication.file %></p>
   </div>
	<div class="panel-footer">
		<%= link_to "View more details",admin_publication_path(publication),class: "btn btn-default" %>
		<%= link_to "Edit",edit_admin_publication_path(publication),class: "btn btn-default" %>
	</div>
</div>
<% end %>